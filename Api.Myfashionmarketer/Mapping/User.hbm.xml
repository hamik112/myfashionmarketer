<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Myfashion.Domain"
      assembly="Domain.Myfashion">

  <class name="User" table="User" lazy="false">
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>

    <property name="Company_id">
      <column name="Company_id" />
    </property>

    <property name="Title">
      <column name="Title" />
    </property>
    <property name="UserName">
      <column name="UserName" />
    </property>
    <property name="EmailId">
      <column name="EmailId"  />
    </property>

    <property name="Phone">
      <column name="Phone"  />
    </property>

    <property name="ProfileUrl">
      <column name="ProfileUrl"  />
    </property>
    <property name="AccountType">
      <column name="AccountType"  />
    </property>


    <property name="CreateDate">
      <column name="CreateDate"  />
    </property>

    <property name="ExpiryDate">
      <column name="ExpiryDate"  />
    </property>

    <property name="UserStatus">
      <column name="UserStatus"  />
    </property>
    <property name="Password">
      <column name="Password"  />
    </property>

    <property name="TimeZone">
      <column name="TimeZone"  />
    </property>

    <property name="PaymentStatus">
      <column name="PaymentStatus"  />
    </property>

    <property name="ActivationStatus">
      <column name="ActivationStatus"  />
    </property>





    <property name="UserType">
      <column name="UserType"  />
    </property>

    <property name="ChangePasswordKey">
      <column name="ChangePasswordKey"  />
    </property>

    <property name="IsKeyUsed">
      <column name="IsKeyUsed"  />
    </property>

    <property name="Company">
      <column name="Company"  />
    </property>
    <property name="Address">
      <column name="Address"  />
    </property>
    <property name="City">
      <column name="City"  />
    </property>
    <property name="Postal">
      <column name="Postal"  />
    </property>
    <property name="PuId">
      <column name="PuId"/>
    </property>
  </class>
</hibernate-mapping>