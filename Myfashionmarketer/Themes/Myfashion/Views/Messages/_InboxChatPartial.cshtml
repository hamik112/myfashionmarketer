@using Domain.Myfashion.Domain;
@using Myfashionmarketer.Api;
@using System.Web.Script.Serialization;
@{
    Domain.Myfashion.Domain.User user = (Domain.Myfashion.Domain.User)Session["User"];
    List<Domain.Myfashion.Domain.TwitterDirectMessages> lstTwitterDirectMessages = (List<Domain.Myfashion.Domain.TwitterDirectMessages>)Model;
    
    }

@foreach(Domain.Myfashion.Domain.TwitterDirectMessages msg in lstTwitterDirectMessages)
{
<li class="you">
        <a href="#" class="user">
            <img src="@msg.SenderProfileUrl" alt="">
        </a>
        <div class="date"><a data-toggle="tooltip" title="">@msg.SenderScreenName</a> @msg.CreatedDate</div>
        <div class="message" style="margin-left:83px">
            <p id="msg_@msg.Id" onclick="opnchatmsg('@msg.SenderId','@msg.RecipientId')" data-target="#chatModal" data-toggle="modal">@msg.Message
           @if (!string.IsNullOrEmpty(msg.Image))
            {
            <img src="@msg.Image" />
            }
                </p>
            <div class="pull-right chatLinks col-md-2">
                <a title="Task" data-target="#task" data-toggle="modal" onclick="opentask('msg_@msg.Id','@msg.CreatedDate')" style="cursor:pointer"><i class="fa fa-thumb-tack"></i></a>
                @*<a title="Email Message" data-target="#msgmailpopup" data-toggle="modal" onclick="MailPopUpMsg('@msg.Id');" style="cursor:pointer"><i class="fa fa-envelope"></i></a>*@
            </div>
        </div>
    </li>
}


